{
	"name": "cube-codes-model",
	"version": "1.0.0",
	"description": "Model describing a Rubik cube and others",
	"main": "dist/package/index.js",
	"types": "dist/package/index.d.ts",
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"browser": "rollup -c",
		"docs": "typedoc",
		"full": "npm ci && npm run build && npm run test && npm run browser && npm run docs",
		"docs-zip": "cd dist/docs && bestzip ../docs.zip * && cd ../..",
		"release": "npm run full && npm run docs-zip && npm publish"
	},
	"files": [
		"dist/package"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/@cube-codes"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cube-codes/cube-codes-model.git"
	},
	"keywords": [
		"Rubik",
		"Cube",
		"Algorithm",
		"Code",
		"Program",
		"Solution"
	],
	"author": "Lukas Lentner <kontakt@lukaslentner.de>",
	"license": "GPL-3.0-only",
	"bugs": {
		"url": "https://github.com/cube-codes/cube-codes-model/issues"
	},
	"homepage": "https://github.com/cube-codes/cube-codes-model#readme",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^15.0.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@types/jest": "^26.0.13",
		"bestzip": "^2.1.7",
		"jest": "^26.4.2",
		"rollup": "^2.26.11",
		"ts-jest": "^26.3.0",
		"typedoc": "^0.17.8",
		"typedoc-plugin-external-module-name": "^4.0.3",
		"typescript": "^4.0.2"
	},
	"dependencies": {
		"@types/deep-equal": "^1.0.1",
		"@types/mathjs": "^6.0.5",
		"deep-equal": "^2.0.3",
		"mathjs": "^7.2.0"
	}
}
