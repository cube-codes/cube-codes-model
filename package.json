{
	"name": "@cube-codes/cube-codes-model",
	"version": "1.0.0",
	"description": "Typescript model of a rubik cube",
	"type": "commonjs",
	"main": "dist/package/index.js",
	"types": "dist/package/index.d.ts",
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"build-browser": "rollup -c",
		"build-docs": "typedoc && node node_modules/nearley/bin/nearley-railroad.js 'src/Cube Move/CubeMoveGrammar.ne' -o 'dist/docs/CubeMoveGrammar.html'",
		"ci-build": "npm ci && npm run build && npm run test && npm run build-browser && npm run build-docs",
		"ci-release": "npm run ci-build && npm publish --access public"
	},
	"files": [
		"dist/package"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cube-codes/cube-codes-model.git"
	},
	"keywords": [
		"Rubik",
		"Cube",
		"Algorithm",
		"Code",
		"Program",
		"Solution"
	],
	"author": "Lukas Lentner <kontakt@lukaslentner.de>",
	"license": "GPL-3.0-only",
	"bugs": {
		"url": "https://github.com/cube-codes/cube-codes-model/issues"
	},
	"homepage": "http://model.cube.codes",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^16.0.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@types/jest": "^26.0.21",
		"jest": "^26.6.3",
		"rollup": "^2.42.1",
		"ts-jest": "^26.5.4",
		"typedoc": "^0.20.32",
		"typescript": "^4.2.3"
	},
	"dependencies": {
		"@thi.ng/bitstream": "^1.1.38",
		"@types/mathjs": "^6.0.11",
		"@types/nearley": "^2.11.1",
		"js-base64": "^3.6.0",
		"mathjs": "^9.3.0",
		"nearley": "^2.20.1"
	}
}
